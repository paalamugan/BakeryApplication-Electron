{
  "name": "bakery-application-electron",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxyconfig.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev": "nodemon ./server/bin/www",
    "prod": "NODE_ENV=production node ./server/bin/www",
    "serve": "ng build --prod && npm run prod",
    "build-js": "browserify --include-path js ./server/bin/www > build/js/script.min.js",
    "electron": "ng build && electron .",
    "electron-aot": "npm run serve && electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=electron/assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . BakeryApplication --overwrite --asar=true --platform=win32 --arch=ia32 --icon=electron/assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"BakeryApp\"",
    "package-linux": "electron-packager . BakeryApplication --overwrite --asar=true --platform=linux --arch=x64 --icon=electron/assets/icons/linux/icon.png --prune=true --out=release-builds"
  },
  "author": "Paalamugan",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^6.0.2",
    "@angular/cdk": "^6.0.2",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/material": "^6.0.2",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@types/rsvp": "^4.0.1",
    "async": "^2.6.1",
    "body-parser": "^1.18.3",
    "browserify": "^16.2.2",
    "core-js": "^2.5.4",
    "cors": "^2.8.5",
    "electron": "^9.1.0",
    "epub.js": "^0.2.15",
    "express": "^4.16.3",
    "express-promise-router": "^3.0.2",
    "hammerjs": "^2.0.8",
    "http-errors": "^1.6.3",
    "jquery": "^3.3.1",
    "mongoose": "^5.1.6",
    "morgan": "^1.9.0",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.3",
    "paper-css": "^0.4.1",
    "sequelize": "^4.38.0",
    "serve-favicon": "^2.5.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "~6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.1.0",
    "codelyzer": "~4.3.0",
    "electron-packager": "^12.1.0",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^1.1.0",
    "nodemon": "^1.17.5",
    "protractor": "~5.3.0",
    "ts-node": "~6.0.3",
    "tslint": "~5.10.0",
    "typescript": "^2.7.2"
  },
  "engines": {
    "node": "v12.0.0",
    "npm": "6.9.0",
    "angular-cli": "6.0.0"
  }
}
